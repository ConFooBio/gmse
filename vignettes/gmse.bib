month = {oct},
number = {10},
pages = {2097--2108},
pmid = {16405155},
title = {{Female mate-choice behavior and sympatric speciation}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16405155},
volume = {59},
year = {2005}
}
