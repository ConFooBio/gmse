journal = {Philosophical Transactions of the Royal Society B},
keywords = {begging behaviour,evolutionarily stable strategy,offspring conflict,parent,parental investment,sib competition},
number = {1419},
pages = {295--307},
pmid = {11958698},
title = {{Intrafamilial conflict and parental investment: a synthesis.}},
