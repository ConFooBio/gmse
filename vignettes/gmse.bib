keywords = {Competitive polymerase chain reaction,Cryptic female choice,Inbreeding avoidance,Sperm storage,Teleogryllus oceanicus},
number = {6},
pages = {1261--1268},
pmid = {23745826},
title = {{Female crickets assess relatedness during mate guarding and bias storage of sperm towards unrelated males}},
volume = {26},
