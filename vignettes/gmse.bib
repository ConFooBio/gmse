journal = {Animal Behaviour},
keywords = {Extrapair mating,Heterozygosity,Inbreeding avoidance,Malurus melanocephalus,Red-backed fairy-wren,Relatedness},
number = {3},
pages = {857--864},
publisher = {Elsevier Ltd},
title = {{Extrapair copulations reduce inbreeding for female red-backed fairy-wrens, Malurus melanocephalus}},
