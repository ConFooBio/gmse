keywords = {Alleles,Biological Evolution,Chromosome Mapping,Fertilization,Fertilization: genetics,Genetic,Genetic: genetics,Inbreeding,Models,Mutation,Mutation: genetics,Plants,Plants: genetics,Recombination,Selection},
month = {aug},
number = {1},
pages = {14--46},
pmid = {1948770},
title = {{Coevolution of self-fertilization and inbreeding depression. I. Mutation-selection balance at one and two loci}},
