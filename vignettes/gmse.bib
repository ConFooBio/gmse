journal = {FEMS Yeast Research},
keywords = {Australia,Cluster Analysis,DNA,Europe,Evolution,Fungal,Fungal: chemistry,Fungal: genetics,Genetic,Genotype,Molecular,Molecular Sequence Data,New Zealand,Phylogeny,Polymorphism,Quercus,Quercus: microbiology,Recombination,Saccharomyces,Saccharomyces: classification,Saccharomyces: genetics,Saccharomyces: isolation {\&} purification,Sequence Analysis},
month = {nov},
number = {7},
pages = {941--947},
pmid = {20868381},
title = {{Saccharomyces paradoxus and Saccharomyces cerevisiae reside on oak trees in New Zealand: evidence for migration from Europe and interspecies hybrids}},
