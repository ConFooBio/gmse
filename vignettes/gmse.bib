@article{Rosenfeld2009,
abstract = {The question of dynamical stability and stochastic behavior of large$\backslash$nbiochemical networks is discussed. It is argued that stringent conditions$\backslash$nof asymptotic stability have very little chance to materialize in$\backslash$na multidimensional system described by the differential equations$\backslash$nof chemical kinetics. The reason is that the criteria of asymptotic$\backslash$nstability (Routh-Hurwitz, Lyapunov criteria, Feinberg's Deficiency$\backslash$nZero theorem) would impose the limitations of very high algebraic$\backslash$norder on the kinetic rates and stoichiometric coefficients, and there$\backslash$nare no natural laws that would guarantee their unconditional validity.$\backslash$nHighly nonlinear, dynamically unstable systems, however, are not$\backslash$nnecessarily doomed to collapse, as a simple Jacobian analysis would$\backslash$nsuggest. It is possible that their dynamics may assume the form of$\backslash$npseudo-random fluctuations quite similar to a shot noise, and, therefore,$\backslash$ntheir behavior may be described in terms of Langevin and Fokker-Plank$\backslash$nequations. We have shown by simulation that the resulting pseudo-stochastic$\backslash$nprocesses obey the heavy-tailed Generalized Pareto Distribution with$\backslash$ntemporal sequence of pulses forming the set of constituent-specific$\backslash$nPoisson processes. Being applied to intracellular dynamics, these$\backslash$nproperties are naturally associated with burstiness, a well documented$\backslash$nphenomenon in the biology of gene expression.},
author = {Rosenfeld, Simon},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rosenfeld - 2009 - Patterns of stochastic behavior in dynamically unstable high-dimensional biochemical networks.pdf:pdf},
isbn = {1177-6250 (Electronic)$\backslash$n1177-6250 (Linking)},
issn = {11776250},
journal = {Gene Regulation and Systems Biology},
keywords = {Asymptotic stability,Biochemical networks,Gene expression,Nonlinear dynamics,Stochasticity},
pages = {1--10},
pmid = {19838330},
title = {{Patterns of stochastic behavior in dynamically unstable high-dimensional biochemical networks}},
volume = {3},
year = {2009}
}
@article{Rosenheim2008,
abstract = {For more than 80 years, ecologists have debated whether reproduction by female insect herbivores and parasitoids is constrained by the time needed to find hosts (time limitation) or by the finite supply of mature eggs (egg limitation). Here we present the first direct measures of permanent time limitation and egg limitation and their influences on the cost of oviposition and lifetime reproduction for an insect in nature. We studied the gall midge Rhopalomyia californica, which neither matures nor resorbs eggs during the adult stage. By sampling females soon after their death and correcting for predation effects, we demonstrate that females lay a large proportion of their total complement of eggs (multiyear mean: 82.9{\%}). The egg supplies of 17.1{\%} of females were completely exhausted, with the remaining 82.9{\%} of females being time limited. As predicted by theory, we estimate that even though egg limitation is a minority condition within the population, egg costs make a substantial contribution (57{\%} of the total) to the cost of oviposition. We conclude that insect life histories evolve to produce a balanced risk of time and egg limitation and, therefore, that both of these constraining factors have important influences on insect oviposition behavior and population dynamics.},
author = {Rosenheim, Jay a and Jepsen, Sarina J and Matthews, Christopher E and Smith, D Solance and Rosenheim, Micah R},
doi = {10.1086/591677},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rosenheim et al. - 2008 - Time limitation, egg limitation, the cost of oviposition, and lifetime reproduction by an insect in nature.pdf:pdf},
issn = {1537-5323},
journal = {American Naturalist},
keywords = {Animals,Diptera,Diptera: growth {\&} development,Diptera: physiology,Female,Oviposition,Reproduction},
month = {oct},
number = {4},
pages = {486--496},
pmid = {18793093},
title = {{Time limitation, egg limitation, the cost of oviposition, and lifetime reproduction by an insect in nature}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18793093},
volume = {172},
year = {2008}
}
@article{Rosindell2011,
abstract = {A decade has now passed since Hubbell published The Unified Neutral Theory of Biodiversity and Biogeography. Neutral theory highlights the importance of dispersal limitation, speciation and ecological drift in the natural world and provides quantitative null models for assessing the role of adaptation and natural selection. Significant advances have been made in providing methods for understanding neutral predictions and comparing them with empirical data. In this review, we describe the current state-of-the-art techniques and ideas in neutral theory and how these are of relevance to ecology. The future of neutral theory is promising, but its concepts must be applied more broadly beyond the current focus on species-abundance distributions. {\textcopyright} 2011 Elsevier Ltd.},
