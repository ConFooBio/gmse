issn = {09609822},
journal = {Current Biology},
keywords = {EVO{\_}ECOL},
number = {23},
pages = {2061--2066},
pmid = {17997307},
publisher = {Elsevier Ltd},
title = {{The genetic basis of inbreeding avoidance in house mice}},
