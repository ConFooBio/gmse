issn = {2041-1723},
journal = {Nature Communications},
keywords = {Animals,Birds,Birds: genetics,Birds: physiology,Finches,Genetics,Inbreeding,Pedigree,Polymorphism,Population,Single Nucleotide,Single Nucleotide: genetics},
month = {jan},
