@book{Skidmore1991,
abstract = {The cow-dung community of organisms plays a vital role in the nutrient cycles and productivity of pastures. While many insects are essential for the rapid conversion of dung into humus, others have a more negative role, being predators or parasitoids or may be vectors of cattle diseases. The present handbook is designed to be of use both to specialists and to beginners for appropriate studies in the British Isles. A brief introduction outlines basic issues, sampling techniques and specimen preparation. Part I first keys out the major groups of invertebrates as well as some dung-frequenting plants; insect eggs, larvae and pupae are described here and insects are keyed down to family level. Part II, the longest part of the book, then keys the Coleoptera, Diptera and Hymenoptera of ungulate dung to species level, adding considerable biological information. Part III covers dipteran families which occur as casuals in cow dung, and Part IV deals with the cow-dung community with its trophic groups and its role in the nutrient cycle in pastures. The book concludes with a taxonomic list of insects associated with the dung of various ungulates (cow, sheep, deer, horse, and unspecified), a bibliography to appropriate ecological and taxonomic literature, a glossary and an index.},
author = {Skidmore, Peter},
file = {:home/brad/Dropbox/DuthiePapers/Skidmore1991.pdf:pdf},
isbn = {1-85153-821-6},
