journal = {Journal of Ecology},
keywords = {Amazonian landscape,Climatic and soil gradients,Determinants of plant community diversity and stru,Environmental filtering,Functional traits,Tree communities,Tropical forests},
number = {1},
pages = {145--155},
title = {{Environmental factors predict community functional composition in Amazonian forests}},
volume = {102},
