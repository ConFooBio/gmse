issn = {1558-5646},
journal = {Evolution},
keywords = {Animal,Animal: physiology,Animals,Biological Evolution,Genetic,Genetic Variation,Hermaphroditic Organisms,Hermaphroditic Organisms: genetics,Inbreeding,Plants,Reproduction,Selection,Self-Fertilization,Sexual Behavior,Snails,Snails: genetics,Snails: physiology},
