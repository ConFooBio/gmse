file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kerdelhue, Le Clainche, Rasplus - 1999 - Molecular phylogeny of the Ceratosolen species pollinating Ficus of the subgenus Sycomorus sens.pdf:pdf},
isbn = {1055-7903},
issn = {1055-7903},
journal = {Molecular Phylogenetics and Evolution},
number = {3},
pages = {401--414},
pmid = {10196081},
title = {{Molecular phylogeny of the Ceratosolen species pollinating Ficus of the subgenus Sycomorus sensu stricto: biogeographical history and origins of the species-specificity breakdown cases}},
volume = {11},
year = {1999}
}
