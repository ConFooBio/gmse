journal = {Evolution},
keywords = {Alleles,Angiosperms,Angiosperms: genetics,Evolution,Genes,Genetic,Homozygote,Models,Molecular,Mutation Rate,Plant,Recessive,Self-Fertilization,Self-Fertilization: genetics},
month = {dec},
