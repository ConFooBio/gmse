file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Rheindt et al. - 2004 - Extremely high mating frequency in the Florida harvester ant (Pogonomyrmex badius).pdf:pdf},
isbn = {ISSN 0340-5443},
issn = {03405443},
journal = {Behavioral Ecology and Sociobiology},
keywords = {Effective mating frequency,Multiple mating,Pogonomyrmex badius,Polyandry},
pages = {472--481},
title = {{Extremely high mating frequency in the Florida harvester ant (Pogonomyrmex badius)}},
