journal = {Molecular Ecology},
keywords = {Animals,Base Sequence,Central America,Cluster Analysis,DNA,DNA Primers,Ecosystem,Ficus,Ficus: genetics,Ficus: parasitology,Genetic,Genetic Speciation,Host-Parasite Interactions,Likelihood Functions,Mitochondrial,Mitochondrial: genetics,Models,Molecular Sequence Data,Phylogeny,Sequence Analysis,Species Specificity,Symbiosis,United States,Wasps,Wasps: genetics},
month = {may},
