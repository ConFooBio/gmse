keywords = {Altruism,Animals,Chiroptera,Chiroptera: genetics,Chiroptera: physiology,Cooperative Behavior,Feeding Behavior,Female,Genetic,Genotype,Male,Microsatellite Repeats,Selection,Videotape Recording},
