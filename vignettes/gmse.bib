issn = {2051-1310},
journal = {Journal of Complex Networks},
keywords = {bow-tie networks,core,nested networks,onion networks,periphery networks,rich-club},
number = {2},
pages = {93--123},
title = {{Structure and dynamics of core/periphery networks}},
url = {https://academic.oup.com/comnet/article-lookup/doi/10.1093/comnet/cnt016},
volume = {1},
year = {2013}
}
