doi = {10.1111/j.1558-5646.2007.00184.x},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Steets et al. - 2007 - The role of natural enemies in the expression and evolution of mixed mating in hermaphroditic plants and animals.pdf:pdf},
