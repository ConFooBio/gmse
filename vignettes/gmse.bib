journal = {Evolution},
keywords = {Acrocephalus arundinaceus,DNA fingerprinting,band sharing,great reed warbler,hatching failure,nonincestuous inbreeding,population bottleneck,reproductive success.},
number = {2},
pages = {317--326},
pmid = {395},
title = {{Genetic similarity between parents predicts hatching failure: nonincestuous inbreeding in the great reed warbler?}},
url = {http://www.jstor.org.ezproxy.webfeat.lib.ed.ac.uk/stable/pdfplus/2410095.pdf},
