pmid = {24299413},
title = {{Evaluating a simple approximation to modeling the joint evolution of self-fertilization and inbreeding depression}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24299413},
volume = {67},
year = {2013}
}
