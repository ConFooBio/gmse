pages = {041301},
title = {{From Planck data to Planck era: Observational tests of Holographic Cosmology}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2016arXiv160704878A{\&}link{\_}type=ABSTRACT{\%}5Cnpapers://ee00755c-a478-4d4e-a50b-ef01ee7b9957/Paper/p37774},
