issn = {0303-6812},
journal = {Journal of Mathematical Biology},
keywords = {Alleles,Animals,Biological Evolution,Competitive Behavior,Computer-Assisted,Genetic,Genetic Variation,Linkage Disequilibrium,Models,Numerical Analysis,Quantitative Trait Loci,Quantitative Trait Loci: genetics,Selection},
