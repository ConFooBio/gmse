keywords = {Animal,Animals,Drosophila melanogaster,Drosophila melanogaster: anatomy {\&} histology,Drosophila melanogaster: genetics,Drosophila melanogaster: physiology,Female,Inbreeding,Male,Mating Preference,Multivariate Analysis,Sex Characteristics,Sexual Behavior},
