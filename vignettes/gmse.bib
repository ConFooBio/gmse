title = {{The links between ecosystem multifunctionality and above- and belowground biodiversity are mediated by climate.}},
url = {http://www.nature.com/ncomms/2015/150902/ncomms9159/full/ncomms9159.html{\#}methods},
volume = {6},
year = {2015}
}
