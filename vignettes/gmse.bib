journal = {American Naturalist},
keywords = {and thus natural selection,f statistics,fit than,inbreeding,inbreeding avoidance,kinship,matings are often more,pedigree,random mating,the progeny of outbred,those of inbred matings},
month = {sep},
number = {3},
pages = {380--392},
pmid = {18811446},
title = {{No evidence for inbreeding avoidance in a natural population of song sparrows (Melospiza melodia).}},
