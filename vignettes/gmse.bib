journal = {Journal of Applied Ecology},
keywords = {Conflict,Dialogue,Grouse,Harrier,Predators,Stakeholder,Uplands},
number = {5},
pages = {961--963},
title = {{Hen harriers and red grouse: Moving towards consensus?}},
volume = {46},
