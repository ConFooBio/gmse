issn = {0960-9822},
journal = {Current Biology},
keywords = {Adaptation,Animal,Animals,Behavior,Cichlids,Cichlids: genetics,Cichlids: physiology,Crosses,Female,Genetic,Inbreeding,Male,Mating Preference,Physiological,Reproduction},
month = {feb},
