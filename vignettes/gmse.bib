@article{Tregenza2002,
abstract = {Why do females typically mate with more than one male? Female mating patterns have broad implications for sexual selection, speciation and conflicts of interest between the sexes, and yet they are poorly understood. Matings inevitably have costs, and for females, the benefits of taking more than one mate are rarely obvious. One possible explanation is that females gain benefits because they can avoid using sperm from genetically incompatible males, or invest less in the offspring of such males. It has been shown that mating with more than one male can increase offspring viability, but we present the first clear demonstration that this occurs because females with several mates avoid the negative effects of genetic incompatibility. We show that in crickets, the eggs of females that mate only with siblings have decreased hatching success. However, if females mate with both a sibling and a non-sibling they avoid altogether the low egg viability associated with sibling matings. If similar effects occur in other species, inbreeding avoidance may be important in understanding the prevalence of multiple mating.},
annote = {A key hypothesis here is that males have higher reproductive success when they mate with non-relatives. This hyothesis is tested in our model `Polyandry II', in which offspring paternity is allocated by male quality, as determined by inbreeding strategy (so females that avoid inbreeding bias paternity towards non-relatives).

Females that mate with a sibling and a non-sibling have the same egg--hatching proportions as females that mate with two non-siblings (ca 0.55 vs. ca 0.35), implying that polyandry can eliminate the effects of inbreeding (presumably because paternity allocated to eggs is skewed towards non-siblings). Like all previous studies, this does not really get to individual fitness because females may increase their inclusive fitness by inbreeding. Further, in a small wild population where polyandry might evolve to avoid inbreeding, the choice is not going to typically be to mate with a full sibling versus a non-relative.

Note that this does not consider the initial choice of a focal female (i.e., if she could avoid inbreeding in her first mating).

This study claims to be the first to find that polyandry helps females avoid the negative fitness consequences of inbreeding.},
author = {Tregenza, Tom and Wedell, Nina},
doi = {10.1038/415071a},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tregenza, Wedell - 2002 - Polyandrous females avoid costs of inbreeding.pdf:pdf},
isbn = {0028-0836},
issn = {00280836},
journal = {Nature},
keywords = {Animal,Animal: physiology,Animals,Botswana,Courtship,Ecology,Female,Fertility,Fertility: physiology,Fertilization,Fertilization: physiology,Genetic,Gryllidae,Gryllidae: physiology,Inbreeding,Male,Oviposition,Oviposition: physiology,Reproduction,Reproduction: physiology,Selection,Sexual Behavior},
month = {jan},
number = {6867},
pages = {71--73},
pmid = {11780118},
title = {{Polyandrous females avoid costs of inbreeding}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11780118},
volume = {415},
year = {2002}
}
