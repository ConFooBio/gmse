doi = {10.1111/jeb.12118},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tuni, Beveridge, Simmons - 2013 - Female crickets assess relatedness during mate guarding and bias storage of sperm towards unrelated ma.pdf:pdf},
isbn = {1420-9101},
