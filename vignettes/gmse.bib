isbn = {1045-2249},
issn = {10452249},
journal = {Behavioral Ecology},
keywords = {Araneidae,Genetic benefits,Polyandry,Trade up},
number = {5},
pages = {1056--1062},
title = {{Inbreeding avoidance through cryptic female choice in the cannibalistic orb-web spider Argiope lobata}},
volume = {20},
year = {2009}
}
