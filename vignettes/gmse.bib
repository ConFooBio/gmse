journal = {BioSystems},
keywords = {Adaptation,Cryptic genetic variation,Degeneracy,Evolvability,Selection},
number = {1},
pages = {34--42},
pmid = {22910487},
publisher = {Elsevier Ireland Ltd},
title = {{Degeneracy allows for both apparent homogeneity and diversification in populations}},
