journal = {American Naturalist},
month = {aug},
number = {2},
pages = {109--123},
pmid = {18707340},
title = {{Can the evolution of plant defense lead to plant-herbivore mutualism?}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18707340},
volume = {158},
year = {2001}
}
