keywords = {ADAPTIVE AGENTS,COMPLEXITY,DECISION-MAKING,HOMO-ECONOMICUS,LAND-USE CHANGE,MULTIAGENT SYSTEMS,SIMULATION,SPATIAL MODELS,SPECIAL-ISSUE,ZERO-INTELLIGENCE TRADERS,decision making,emergent properties,multi-agent systems,validation},
pages = {39--53},
pmid = {20146761},
title = {{Agent-based modeling in ecological economics}},
url = {http://apps.webofknowledge.com/full{\_}record.do?product=WOS{\&}search{\_}mode=GeneralSearch{\&}qid=11{\&}SID=S1rGoF2cTZcl7gwRY7c{\&}page=1{\&}doc=1},
