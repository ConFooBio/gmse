volume = {26},
year = {2011}
}
@article{Schultz2016,
abstract = {The effects from multigenerational exposures to engineered nanoparticles$\backslash$n(ENPs) in their pristine and transformed states are currently unknown$\backslash$ndespite such exposures being an increasingly common scenario in natural$\backslash$nenvironments. Here, we examine how exposure over 10 generations affects$\backslash$nthe sensitivity of the nematode Caenorhabditis elegans to pristine and$\backslash$nsulfidized Ag ENPs and AgNO3. We also include populations that were$\backslash$ninitially exposed over six generations but kept unexposed for subsequent$\backslash$nfour generations to allow recovery from exposure. Toxicity of the$\backslash$ndifferent silver forms decreased in the order AgNO3, Ag ENPs and Ag2S$\backslash$nENPs. Continuous exposure to Ag ENPs and AgNO3 caused pronounced$\backslash$nsensitization (approx. 10-fold) in the F2 generation, which was$\backslash$nsustained until F10. This sensitization was less pronounced for Ag2S ENP$\backslash$nexposures, indicating different toxicity mechanisms. Subtle changes in$\backslash$nsize and lifespan were also measured. In the recovery populations, the$\backslash$nsensitivity to Ag ENPs and AgNO3 resulting from the initial$\backslash$nmultigenerational exposure persisted. Their response sensitivity for all$\backslash$nendpoints was most closely related to the last ancestral exposed$\backslash$ngeneration (F5), rather than unexposed controls. The mechanisms of$\backslash$ntransgenerational transfer of sensitivity are probably organized through$\backslash$nthe epigenome, and we encourage others to investigate such effects as a$\backslash$npriority for mechanistic toxicology.},
author = {Schultz, Carolin L. and Wamucho, Anye and Tsyusko, Olga V. and Unrine, Jason M. and Crossley, Alison and Svendsen, Claus and Spurgeon, David J.},
