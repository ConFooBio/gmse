keywords = {Animals,Biological,Ficus,Ficus: parasitology,Ficus: physiology,Models,Organ Size,Ovum,Ovum: cytology,Ovum: parasitology,Ovum: physiology,Parasites,Parasites: physiology,Pollination,Pollination: physiology,Seeds,Seeds: parasitology,Seeds: physiology,Symbiosis,Symbiosis: physiology,Wasps,Wasps: physiology},
