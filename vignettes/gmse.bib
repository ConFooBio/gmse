journal = {Vaccine},
keywords = {Adjuvants,Antibodies,Clinical Trials as Topic,H1N1 Subtype,H1N1 Subtype: immunology,Hemagglutination Inhibition Tests,Human,Human: immunology,Human: prevention {\&} control,Humans,Immunologic,Immunologic: administration {\&} dosage,Immunologic: adverse effects,Inactivated,Inactivated: administration {\&} dosage,Inactivated: adverse effects,Inactivated: immunology,Influenza,Influenza A Virus,Influenza Vaccines,Influenza Vaccines: administration {\&} dosage,Influenza Vaccines: adverse effects,Influenza Vaccines: immunology,Vaccines,Viral,Viral: blood},
