issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences},
pmid = {28566499},
title = {{Finally making sense of the double-slit experiment}},
url = {http://www.nature.com/doifinder/10.1038/nature22898},
year = {2017}
}
