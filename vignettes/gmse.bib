issn = {2154-1256},
journal = {Small GTPases},
keywords = {abbreviations,adp-ribosylation,arf,arf gtpases,brag1,factor,gef,genetics,guanine nucleotide,iq-arfgef,iqsec2},
number = {2},
pages = {98--103},
pmid = {21686261},
title = {{Subtle functional defects in the Arf-specific guanine nucleotide exchange factor IQSEC2 cause non-syndromic X-linked intellectual disability.}},
