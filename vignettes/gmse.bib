journal = {Proceedings of the National Academy of Sciences},
number = {27},
pages = {7575--7579},
pmid = {27325781},
title = {{Global threat to agriculture from invasive species}},
url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1602205113},
volume = {113},
year = {2016}
}
