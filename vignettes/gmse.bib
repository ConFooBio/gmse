issn = {0269-7653},
journal = {Evolutionary Ecology},
keywords = {Bumpus,LISREL,Passer domesticus,evolution,fitness,house sparrow,natural selection,phenotypic selection,structural equation model,structural equation modelling,survival},
number = {4},
pages = {387--404},
title = {{The Bumpus house sparrow data: A reanalysis using structural equation models}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0029659645{\&}partnerID=40{\&}md5=2116e6d3349deca1ba2b871a7f215dfa},
volume = {10},
year = {1996}
}
@article{Puniamoorthy2012,
abstract = {Sexual size dimorphism (SSD) varies widely across and within species. The differential equilibrium model of SSD explains dimorphism as the evolutionary outcome of consistent differences in natural and sexual selection between the sexes. Here, we comprehensively examine a unique cross-continental reversal in SSD in the dung fly, Sepsis punctum. Using common garden laboratory experiments, we establish that SSD is male-biased in Europe and female-biased in North America. When estimating sexual (pairing success) and fecundity selection (clutch size of female partner) on males under three operational sex ratios (OSRs), we find that the intensity of sexual selection is significantly stronger in European versus North American populations, increasing with male body size and OSR in the former only. Fecundity selection on female body size also increases strongly with egg number and weakly with egg volume, however, equally on both continents. Finally, viability selection on body size in terms of intrinsic (physiological) adult life span in the laboratory is overall nil and does not vary significantly across all seven populations. Although it is impossible to prove causality, our results confirm the differential equilibrium model of SSD in that differences in sexual selection intensity account for the reversal in SSD in European versus North American populations, presumably mediating the ongoing speciation process in S. punctum.},
author = {Puniamoorthy, Nalini and Sch{\"{a}}fer, Martin A and Blanckenhorn, Wolf U},
doi = {10.1111/j.1558-5646.2012.01599.x},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Puniamoorthy, Sch{\"{a}}fer, Blanckenhorn - 2012 - Sexual selection accounts for the geographic reversal of sexual size dimorphism in the dun.pdf:pdf},
isbn = {1558-5646},
issn = {00143820},
journal = {Evolution},
keywords = {Body size,Natural selection,Population differentiation,Sepsid flies,Sexual selection,Speciation},
number = {7},
pages = {2117--2126},
pmid = {22759289},
title = {{Sexual selection accounts for the geographic reversal of sexual size dimorphism in the dung fly, sepsis punctum (diptera: Sepsidae)}},
volume = {66},
year = {2012}
}
