issn = {10958304},
journal = {Appetite},
keywords = {Climate change,Consumers,Local and seasonal food,Meat eating,Mitigation},
pages = {19--27},
pmid = {26673412},
publisher = {Elsevier Ltd},
title = {{Help the climate, change your diet: A cross-sectional study on how to involve consumers in a transition to a low-carbon society}},
url = {http://dx.doi.org/10.1016/j.appet.2015.12.001},
