@article{Takeuchi2016,
abstract = {9 Many textbooks define evolution as changes in the properties of a 10 population over generations. Does this definition always capture the 11 dynamics of evolution? Life is hierarchically organized, and evolution 12 can operate at multiple levels with conflicting tendencies. Using a min-13 imal model of such conflicting multilevel evolution, we demonstrate the 14 possibility of a novel mode of evolution that challenges the above def-15 inition: individuals ceaselessly modify their genetically-inherited phe-16 notype and fitness along their lines of descent, without involving appar-17 ent changes in the properties of the population. The model assumes 18 a population of primitive cells (protocells, for short), each contain-19 ing a population of replicating catalytic molecules. Protocells are se-20 lected towards maximizing the catalytic activity of internal molecules, 21 whereas molecules tend to evolve towards minimizing it in order to 22 maximize their relative fitness within a protocell. These conflicting 23 evolutionary tendencies at different levels and genetic drift drive the 24 lineages of protocells to oscillate endlessly between high and low intra-25 cellular catalytic activity, i.e., high and low fitness, along their lines of 26 descent. This oscillation, however, occurs independently in different 27 lineages, so that the population as a whole appears stationary. There-28 fore, ongoing evolution can be hidden behind an apparently stationary 29 population owing to conflicting multilevel evolution. 30 Keywords: multilevel selection | prebiotic evolution | intragenomic con-31 flict | RNA world | major evolutionary transition},
author = {Takeuchi, Nobuto and Kaneko, Kunihiko and Hogeweg, Paulien},
doi = {10.1098/rspb.2015.3109},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Takeuchi, Kaneko, Hogeweg - 2016 - Evolutionarily stable disequilibrium Endless dynamics of evolution in a stationary population.pdf:pdf},
issn = {1471-2954},
journal = {Proceedings of The Royal Society B},
keywords = {biophysics,evolution,theoretical biology},
pmid = {27147095},
title = {{Evolutionarily stable disequilibrium: Endless dynamics of evolution in a stationary population}},
year = {2016}
}
@article{Tamuri2012,
abstract = {Estimation of the distribution of selection coefficients of mutations is a long-standing issue in molecular evolution. In addition to population-based methods, the distribution can be estimated from DNA sequence data by phylogenetic-based models. Previous models have generally found unimodal distributions where the probability mass is concentrated between mildly deleterious and nearly neutral mutations. Here we use a sitewise mutation-selection phylogenetic model to estimate the distribution of selection coefficients among novel and fixed mutations (substitutions) in a data set of 244 mammalian mitochondrial genomes and a set of 401 PB2 proteins from influenza. We find a bimodal distribution of selection coefficients for novel mutations in both the mitochondrial data set and for the influenza protein evolving in its natural reservoir, birds. Most of the mutations are strongly deleterious with the rest of the probability mass concentrated around mildly deleterious to neutral mutations. The distribution of the coefficients among substitutions is unimodal and symmetrical around nearly neutral substitutions for both data sets at adaptive equilibrium. About 0.5{\%} of the nonsynonymous mutations and 14{\%} of the nonsynonymous substitutions in the mitochondrial proteins are advantageous, with 0.5{\%} and 24{\%} observed for the influenza protein. Following a host shift of influenza from birds to humans, however, we find among novel mutations in PB2 a trimodal distribution with a small mode of advantageous mutations.},
author = {Tamuri, Asif U and dos Reis, Mario and Goldstein, Richard A},
doi = {10.1534/genetics.111.136432},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tamuri, dos Reis, Goldstein - 2012 - Estimating the distribution of selection coefficients from phylogenetic data using sitewise mutatio.pdf:pdf},
issn = {1943-2631},
journal = {Genetics},
keywords = {Algorithms,Animals,Computer Simulation,Evolution,Genetic,Genetic Drift,Humans,Models,Molecular,Mutation,Phylogeny,Reproducibility of Results,Selection},
month = {mar},
number = {3},
pages = {1101--1115},
