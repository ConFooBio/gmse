issn = {10958312},
journal = {Biological Journal of the Linnean Society},
keywords = {Cryptic female choice,Male-male competition,Mating behaviour,Paternity,Polyandry,Red flour beetle,Sexual conflict,Sperm competition},
number = {1},
pages = {67--75},
title = {{Pre- and postcopulatory sexual selection act in concert to determine male reproductive success in Tribolium castaneum}},
