issn = {0029-8549},
journal = {Oecologia},
keywords = {Analysis of Variance,Animals,Diptera,Diptera: physiology,Flowers,Flowers: anatomy {\&} histology,France,Geography,Larva,Larva: physiology,Population Density,Population Dynamics,Predatory Behavior,Predatory Behavior: physiology,Ranunculaceae,Ranunculaceae: anatomy {\&} histology,Ranunculaceae: physiology,Seeds,Sweden,Symbiosis},
