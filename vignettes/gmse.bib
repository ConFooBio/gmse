issn = {1943-2631},
journal = {Genetics},
month = {apr},
number = {July},
pages = {913--923},
pmid = {24770330},
title = {{How to infer relative fitness from a sample of genomic sequences}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24770330},
volume = {197},
year = {2014}
}
