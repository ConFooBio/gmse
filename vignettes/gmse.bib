pages = {151--157},
pmid = {26947271},
publisher = {Elsevier},
title = {{Hamilton׳s Rule in finite populations with synergistic interactions}},
url = {http://dx.doi.org/10.1016/j.jtbi.2016.02.034{\%}5Cnhttp://linkinghub.elsevier.com/retrieve/pii/S0022519316001302},
volume = {397},
year = {2016}
}
