journal = {Current Biology},
keywords = {Animals,Arctic Regions,Bayes Theorem,Biological,Biological Evolution,DNA,Ecosystem,Host-Parasite Interactions,Host-Parasite Interactions: physiology,Mitochondrial,Mitochondrial: genetics,Models,Molecular Sequence Data,Plant Diseases,Plant Diseases: parasitology,Quercus,Quercus: parasitology,Time Factors,Wasps,Wasps: parasitology},
month = {mar},
