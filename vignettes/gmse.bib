doi = {10.1111/j.1469-8137.2012.04383.x},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yu, Nason - 2013 - Nuclear and chloroplast DNA phylogeography of Ficus hirta obligate pollination mutualism and constraints on range exp.pdf:pdf},
issn = {1469-8137},
journal = {New Phytologist},
keywords = {expansion,ficus,gene flow,glacial refugia,how interspecific interactions in,mutualism,phylogeographic approach to investigate,phylogeography,range,southeast asia,this study uses a,tropical forest},
month = {jan},
number = {1},
pages = {276--289},
pmid = {23127195},
title = {{Nuclear and chloroplast DNA phylogeography of Ficus hirta: obligate pollination mutualism and constraints on range expansion in response to climate change}},
