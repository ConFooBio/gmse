journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {environmental science,physiology},
number = {1832},
pages = {20152911},
pmid = {27306046},
title = {{Multigenerational exposure to silver ions and silver nanoparticles reveals heightened sensitivity and epigenetic memory in {\textless}i{\textgreater}Caenorhabditis elegans{\textless}/i{\textgreater}}},
