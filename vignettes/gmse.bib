@article{Mullon2016,
abstract = {The evolutionary stability of quantitative traits depends on whether a population can resist invasion by any mutant. While uninvadability is well understood in well-mixed populations, it is much less so in subdivided populations when multiple traits evolve jointly. Here, we investigate whether a spatially subdivided population at a monomorphic equilibrium for multiple traits can withstand invasion by any mutant, or is subject to diversifying selection. Our model also explores the among traits correlations arising from diversifying selection and how they depend on relatedness due to limited dispersal. We find that selection favours a positive (negative) correlation between two traits, when the selective effects of one trait on relatedness is positively (negatively) correlated to the indirect fitness effects of the other trait. We study the evolution of traits for which this matters: dispersal that decreases relatedness, and helping that has positive indirect fitness effects. We find that when dispersal cost is low and the benefits of helping accelerate faster than its costs, selection leads to the coexistence of mobile defectors and sessile helpers. Otherwise, the population evolves to a monomorphic state with intermediate helping and dispersal. Overall, our results highlight the importance of population subdivision for evolutionary stability and correlations among traits.},
author = {Mullon, Charles and Keller, Laurent and Lehmann, Laurent},
doi = {doi:10.5061/dryad.01f0f},
file = {:home/brad/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mullon, Keller, Lehmann - 2016 - Evolutionary stability of jointly evolving traits in subdivided populations.pdf:pdf},
issn = {0003-0147},
journal = {American Naturalist},
keywords = {Behavior: evolution,Genetics: population,Metapopulations},
number = {2},
pages = {1--40},
title = {{Evolutionary stability of jointly evolving traits in subdivided populations}},
url = {http://dx.doi.org/10.5061/dryad.01f0f},
volume = {188},
year = {2016}
}
