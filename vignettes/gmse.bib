journal = {American Naturalist},
keywords = {agent-based modelling,complexity,error avoidance,model development,modest approach},
number = {5},
pages = {669--674},
title = {{Per aspera ad astra: through complex population modeling to predictive theory}},
url = {http://www.jstor.org/stable/10.1086/683181},
volume = {186},
year = {2015}
}
