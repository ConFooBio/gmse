keywords = {Adaptive Management,Bayesian network,Native vegetation,Process model,Restoration,State-and-transition},
number = {4},
pages = {1244--1235},
publisher = {Elsevier Ltd},
title = {{State-and-transition modelling for Adaptive Management of native woodlands}},
