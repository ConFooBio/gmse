keywords = {Adolescent,Adult,Affect,Affect: physiology,Arousal,Arousal: physiology,Biofeedback,Computer-Assisted,Depressive Disorder,Depressive Disorder: physiopathology,Depressive Disorder: therapy,Electrocardiography,Feasibility Studies,Female,Fourier Analysis,Heart Rate,Heart Rate: physiology,Humans,Male,Middle Aged,Pilot Projects,Psychology,Psychology: physiology,Respiration,Signal Processing,Sympathetic Nervous System,Sympathetic Nervous System: physiopathology,Vagus Nerve,Vagus Nerve: physiopathology,Vasoconstriction,Vasoconstriction: physiology},
number = {4},
pages = {195--201},
