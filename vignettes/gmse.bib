keywords = {Carbon Dioxide,Carbon Dioxide: chemistry,Climate Change,Conservation of Natural Resources,Earth (Planet),Ecology,Fresh Water,Global Warming,Models,Temperature,Theoretical,Water,Water Supply,Water: chemistry},
month = {jan},
number = {1934},
pages = {99--116},
pmid = {21115515},
title = {{Water availability in +2°C and +4°C worlds}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21115515},
