issn = {0962-8452},
journal = {Proceedings of the Royal Society B},
keywords = {Animal,Animal: physiology,Animals,Behavior,Ecosystem,Female,Inbreeding,Male,Social Behavior,Spiders,Spiders: physiology},
number = {1583},
pages = {157--163},
pmid = {16555782},
title = {{Group living and inbreeding depression in a subsocial spider}},
