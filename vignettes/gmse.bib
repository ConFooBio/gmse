journal = {Genetics},
keywords = {Alleles,Computer Simulation,Gene Frequency,Genetic,Genetic Variation,Models,Ne,effective population size,genetic variance},
number = {2},
pages = {379--391},
pmid = {2731727},
title = {{A generalized approach for estimating effective population size from temporal changes in allele frequency}},
