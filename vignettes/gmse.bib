issn = {1471-2970},
journal = {Philosophical Transactions of the Royal Society B},
keywords = {Animal,Animal: physiology,Animals,Biological,Competitive Behavior,Competitive Behavior: physiology,Female,Genetics,Male,Mating Preference,Models,Population,Research,Research: trends,Sexual Behavior,Spermatozoa,Spermatozoa: physiology},
number = {1613},
pages = {20120041},
pmid = {23339233},
title = {{The polyandry revolution}},
