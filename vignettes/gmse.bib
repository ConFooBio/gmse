keywords = {Cortical networks,Network spectra,Oscillatory networks,Random matrices,Stability analysis,Synchronization},
number = {7-9},
pages = {1373--1387},
title = {{Stability and synchronization of random brain networks with a distribution of connection strengths}},
volume = {71},
year = {2008}
}
