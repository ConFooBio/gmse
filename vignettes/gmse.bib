journal = {Evolution},
keywords = {arnqvist and,ests within nearly all,inter-,sexual size dimorphism,sexually reproducing species,the sexes are expected,to have conflicting evolutionary,water striders},
pages = {544--557},
pmid = {22276547},
title = {{Sexual conflict and antagonisic coevolution across water strider populations}},
volume = {66},
year = {2011}
}
