journal = {Theoretical Population Biology},
keywords = {First-principles,Habitat fragmentation,Patch size,Resource subdivision,Species coexistence,Trade-off},
pages = {90--102},
publisher = {Elsevier Inc.},
title = {{Interplay between habitat subdivision and minimum resource requirement in two-species competition}},
